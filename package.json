{
  "name": "semantic-pull-request-pre-fill",
  "version": "0.0.0",
  "private": true,
  "description": "Pre-fill a pull request description and title based on a semantic branch name",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateoguzmana/semantic-pull-request-pre-fill.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Mateo Guzm√°n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mateoguzmana/semantic-pull-request-pre-fill/issues"
  },
  "homepage": "https://github.com/mateoguzmana/semantic-pull-request-pre-fill",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.6",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5"
  }
}
